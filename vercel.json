{
  "version": 2,
  "builds": [
    {
      "src": "wims/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1",
      "headers": {
        "cache-control": "public, max-age=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "wims/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "wims.settings"
  },
  "build": {
    "commands": [
      "python3 manage.py collectstatic --noinput"
    ]
  }
 
}